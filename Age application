cout << "Enter your age: ";
int age;
cin >> age;

int age_in_days = age * 365;
int age_in_hours = age_in_days * 24;
int age_in_minuts = age_in_hours * 60;
int age_in_seconds = age_in_minuts * 60;

cout << age_in_days <<" days\n";
cout << age_in_hours <<" hours\n";
cout << age_in_minuts << " minuts\n";
cout << age_in_seconds << " seconds";
