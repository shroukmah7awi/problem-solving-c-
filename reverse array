int arr[5];
cout << "Enter values: " << endl;
cin >> arr[0];
cin >> arr[1];
cin >> arr[2];
cin >> arr[3];
cin >> arr[4];


cout << "-------------------------------" << endl;

for (int i = 0; i <= 4; i++)
{
	cout << arr[i] << endl;
}

cout << "---------------------------------" << endl;

for (int i = 4; i >= 0; i--)
{
	cout << arr[i] << endl;


}
